@model List<EBook.DataAccess.NetCore.DTO.Book>

<table class="table">
    <thead>
        <tr>
            <th>BookId</th>
            <th>BookName</th>
            <th>PublishDate</th>
             <th>Hành động</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count > 0)
        {
            foreach (var item in Model)
            {
                <tr class="product_index_@item.BookID">

                     <td>@item.BookID</td>
                    <td>@item.BookName</td>
                    <td>@item.PublishDate</td>
                    <td>
                        <a data-productid="@item.BookID" onclick="Product_Delete(@item.BookID)">
                            <i class="fa fa-trash">Xóa</i>
                        </a>
                        |

                    </td>
                    
                </tr>
            }
        }
    </tbody>
</table>